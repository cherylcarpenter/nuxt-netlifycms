{
  "name": "nuxt-netlifycms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "backend": "npx netlify-cms-proxy-server",
    "clean": "rm -fr ./node_modules/.cache",
    "dev": "tailwind:dev && nuxt",
    "build": "yarn tailwind:build && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "storybook": "yarn tailwind:dev && start-storybook -s ./static -p 6006",
    "build-storybook": "yarn tailwind:dev && build-storybook -s ./static",
    "style-dictionary:build": "yarn style-dictionary build --config ./styleDictionary.config.js",
    "tailwind:dev": "yarn --cwd ./postCss postcss",
    "tailwind:build": "yarn --cwd ./postCss postcss:build"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "^0.3.4",
    "babel-preset-vue": "^2.0.2",
    "chart.js": "^3.5.1",
    "core-js": "^3.15.1",
    "css-loader": "4.2.0",
    "netlify-cms-app": "^2.15.45",
    "netlify-cms-proxy-server": "^1.3.20",
    "nuxt": "^2.15.7",
    "storybook": "^6.3.9",
    "vue-head": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/vue": "^6.3.9",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "lodash": "^4.17.21",
    "netlify-cli": "^6.9.28",
    "prettier": "^2.3.2",
    "storybook-addon-deps": "^2.2.0",
    "style-dictionary": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-loader": "^15.9.8"
  }
}
